{
  "extends": [
    "eslint:recommended",
    "plugin:jest/recommended",
    "plugin:jest/style",
    "next/core-web-vitals",
    "prettier"
  ],
  "rules": {
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "no-undef": "warn",
    "no-unused-vars": ["warn", { "varsIgnorePattern": "^_" }],
    "prefer-const": "off"
  }
}
